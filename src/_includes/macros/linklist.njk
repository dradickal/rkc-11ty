{# 
    links = [{ url, icon, title }]
#}
{% macro linklist(links, classname) %}
    <div class="{{ classname }}">
        {% for link in links %}
            <a href="{{ link.url }}" title="{{ link.title }}">
                {% if link.icon %}
                    <span aria-hidden="true">
                        <img src="{{ link.icon }}" />
                    </span>
                    <span class="sr-only">{{ link.title }}</span>
                {% else %}
                    {{ link.title }}
                {% endif %}
            </a>
        {% endfor %} 
    </div>
{% endmacro %}